<?xml version="1.0"?>
<launch>
<!-- Generation of racecar and detectors: camera, lidar, zed -->
  <!-- Convert an xacro and put on parameter server -->
  <param name="robot_description" command="$(find xacro)/xacro --inorder $(find od_description)/urdf/racecar.xacro" />
  <!-- Spawn a robot into Gazebo -->
  <node name="spawn_urdf" pkg="gazebo_ros" type="spawn_model" output="screen" args="-param robot_description -urdf -model racecar -z -0.1" />
  <!-- ros_control racecar launch file--> 
  <include file="$(find od_control)/launch/racecar_control.launch"/>

  <!-- Publish "better odom" topic that is normally generated by the particle filter -->
    <node name="better_odom" pkg="topic_tools" type="relay"
          args="/vesc/odom /pf/pose/odom" /> 

  <!--rf2o_Laser_Odometry-->
  <include file="$(find solabot_navigation)/launch/includes/simulator/racecar_rf2o.xml" />

  <!-- ekf odom_fusion -->
  <node pkg="robot_localization" type="ekf_localization_node" name="ekf_se" clear_params="true" >
      <rosparam command="load" file="$(find solabot_navigation)/params/simulator/racecar_ekf_params.yaml" />
  </node>


</launch>
